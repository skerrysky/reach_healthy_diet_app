<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="healthy.diet">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Material3.DayNight.NoActionBar"
        tools:targetApi="31">
        <activity
            android:name=".CompareMealPlansUI"
            android:exported="false" />
        <activity
            android:name=".CompareMealPlansUI_SelectPlans"
            android:exported="false" />
        <activity
            android:name=".ComparisonResultUI"
            android:exported="false" />
        <activity
            android:name=".DailyMealPlansUI"
            android:exported="false" />
        <activity
            android:name=".DailyMealPlansUI_EnterNewPlan"
            android:exported="false" />
        <activity
            android:name=".FoodNutritionInformationUI"
            android:exported="false" />
        <activity
            android:name=".FoodNutritionInformationUI_EnterNewFood"
            android:exported="false" />
        <activity
            android:name=".ExpectedDailyCaloricIntakeUI"
            android:exported="false" />
        <activity
            android:name=".ExpectedDailyCaloricIntakeUI_EnterExpectedIntake"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>